    },
    "keyword": {
      "foreground": "#81D2CEDD"
    },
    "comment": {
      "foreground": "#BBBBBB66"
    },
    "string": {
      "foreground": "#DD99AADD"
    },
    "number": {
      "foreground": "#EBC88DBB"
    },
    "regexp": {
      "foreground": "#5DB4BDCC"
    },
    "operator": {
