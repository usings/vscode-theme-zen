  "tokenColors": [
    {
      "name": "Base text color",
      "scope": [
        "source"
      ],
      "settings": {
        "foreground": "#BBBBBBDD"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
      ],
      "settings": {
        "foreground": "#BBBBBB66",
      }
    },
    {
      "name": "Language constants",
      "scope": [
        "constant.language",
      ],
      "settings": {
        "foreground": "#81D2CEDD"
      }
    },
    {
      "name": "Numeric constants",
      "scope": [
        "constant.numeric",
      ],
      "settings": {
        "foreground": "#EBC88DBB"
      }
    },
    {
      "name": "Character and escape constants",
      "scope": [
        "constant.character",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#81D2CEDD"
      }
    },
    {
      "name": "Language variables",
      "scope": [
        "variable.language",
      ],
      "settings": {
        "foreground": "#BBBBBB99",
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
      ],
      "settings": {
        "foreground": "#DD99AADD"
      }
    },
    {
      "name": "Regular expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5DB4BDCC"
      }
    },
    {
      "name": "Control keywords",
      "scope": [
        "keyword.control",
      ],
      "settings": {
        "foreground": "#81D2CEDD"
      }
    },
    {
      "name": "At-rules and directives",
      "scope": [
        "keyword.control.at-rule",
        "keyword.control.directive",
      ],
      "settings": {
        "foreground": "#A8CC7CDD"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Other keywords",
      "scope": [
        "keyword.other",
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Storage modifiers",
      "scope": [
        "storage.modifier",
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Functions and methods",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "support.function"
      ],
      "settings": {
        "foreground": "#EFCE89DD"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#86C0FFEE"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#CCCCCCBB"
      }
    },
    {
      "name": "Tag reference",
      "scope": [
        "entity.name.tag.reference"
      ],
      "settings": {
        "foreground": "#81D2CEDD"
      }
    },
    {
      "name": "Punctuation - Accessor",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.accessor"
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Punctuation - Definition",
      "scope": [
        "punctuation.definition",
      ],
      "settings": {
        "foreground": "#BBBBBB77"
      }
    },
    {
      "name": "Punctuation - Separator",
      "scope": [
        "punctuation.separator",
      ],
      "settings": {
        "foreground": "#BBBBBB77"
      }
    },
    {
      "name": "Markup - Headings",
      "scope": [
        "markup.heading",
      ],
      "settings": {
        "foreground": "#81D2CEDD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Strikethrough",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "HTML - Property",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.less",
      ],
      "settings": {
        "foreground": "#BBAAFFDD"
      }
    },
    {
      "name": "XML - Tags",
      "scope": [
        "entity.name.tag.xml"
      ],
      "settings": {
        "foreground": "#BBAAFFDD"
      }
    },
    {
      "name": "JSON - Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#BBBBBBDD"
      }
    },
    {
      "name": "YAML - Keys",
      "scope": [
        "entity.name.tag.yaml",
      ],
      "settings": {
        "foreground": "#BBBBBBDD"
      }
    },
    {
      "name": "Javascript - Variable property",
      "scope": [
        "variable.other.property.ts",
        "variable.other.object.property.js"
      ],
      "settings": {
        "foreground": "#BBAAFFDD"
      }
    },
    {
      "name": "Javascript - Component",
      "scope": [
        "support.class.component.js",
      ],
      "settings": {
        "foreground": "#8EA2F7EE"
      }
    },
    {
      "name": "Javascript - Import statement",
      "scope": [
        "keyword.control.import.ts",
        "keyword.control.from.ts",
        "keyword.control.import.js",
        "keyword.control.from.js"
      ],
      "settings": {
        "foreground": "#BBBBBB99"
      }
    },
    {
      "name": "Vue - Special tags",
      "scope": [
        "entity.name.tag.script.html.vue",
        "entity.name.tag.template.html.vue",
        "entity.name.tag.style.html.vue"
      ],
      "settings": {
        "foreground": "#8EA2F7EE"
      }
    },
    {
      "name": "Invalid and deprecated",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FF8787DD",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": {
      "foreground": "#8EA2F7EE"
