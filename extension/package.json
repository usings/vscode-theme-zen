{
  "name": "theme-zen",
  "type": "module",
  "version": "0.3.0",
  "author": "Joie <leaps@live.com>",
  "publisher": "using",
  "displayName": "Zen Theme",
  "description": "A fleet like theme for VS Code.",
  "icon": "assets/zen.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Zen Dark",
        "uiTheme": "vs-dark",
        "path": "themes/color-zen.json"
      }
    ],
    "iconThemes": [
      {
        "id": "zen-dark",
        "label": "Zen Dark",
        "path": "themes/icon-dark.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usings/vscode-theme-zen",
    "directory": "extension"
  },
  "scripts": {
    "dev": "esno watch scripts/index.ts",
    "build": "esno scripts/index.ts",
    "release": "vsce publish --no-dependencies"
  }
}
